{
  "name": "astroconnect",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "react-app-rewired start",
    "start": "react-native start",
    "test": "react-app-rewired test",
    "lint": "eslint .",
    "build:android": "react-native bundle --entry-file index.js --platform android --dev false --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build:web": "set NODE_OPTIONS=--openssl-legacy-provider && react-app-rewired build"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-firebase/app": "^14.5.1",
    "@react-native-firebase/auth": "^14.5.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "firebase": "^9.6.11",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.1",
    "react-native-gesture-handler": "^2.4.1",
    "react-native-localize": "^2.2.1",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1",
    "react-native-web": "^0.17.7",
    "react-scripts": "4.0.3",
    "@react-native-picker/picker": "^2.4.10"
