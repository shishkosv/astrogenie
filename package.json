{
  "name": "astroconnect",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "react-app-rewired start",
    "start": "react-native start",
    "test": "react-app-rewired test",
    "lint": "eslint .",
    "build:android": "react-native bundle --entry-file index.js --platform android --dev false --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build:web": "cross-env DISABLE_ESLINT_PLUGIN=true NODE_OPTIONS=--openssl-legacy-provider react-app-rewired build",
    "start:web": "react-app-rewired start",
    "test:web": "react-app-rewired test"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-firebase/app": "^14.12.0",
    "@react-native-firebase/auth": "^14.12.0",
    "@react-native-picker/picker": "^2.4.10",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "firebase": "^9.23.0",
    "path-browserify": "^1.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.1",
    "react-native-gesture-handler": "^2.4.1",
    "react-native-localize": "^2.2.1",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-web": "^0.18.12",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/runtime": "^7.17.9",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.38",
    "@types/react-native": "^0.67.6",
    "@types/react-test-renderer": "^17.0.1",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "customize-cra": "^1.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eventemitter3": "^5.0.1",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "metro-react-native-babel-preset": "^0.70.2",
    "os-browserify": "^0.3.0",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "react-test-renderer": "17.0.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "~4.0.0",
    "url": "^0.11.4"
  },
  "resolutions": {
    "@types/react": "^17.0.38",
    "eslint": "^7.32.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "crypto": false,
    "stream": false,
    "assert": false,
    "http": false,
    "https": false,
    "os": false,
    "url": false
  },
  "prettier": {
    "singleQuote": true,
    "endOfLine": "auto",
    "semi": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "ignore",
    "embeddedLanguageFormatting": "off",
    "parser": "typescript"
  }
}
